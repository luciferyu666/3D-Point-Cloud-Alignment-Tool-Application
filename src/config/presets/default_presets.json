{
    "algorithmDefaults": {
      "pointCloudAlignment": {
        "algorithm": "ICP",
        "maxIterations": 100,
        "tolerance": 0.001
      },
      "preprocessing": {
        "noiseReduction": "statistical",
        "samplingMethod": "voxelGrid",
        "samplingSize": 0.01
      }
    },
    "userInterfaceSettings": {
      "theme": "dark",
      "language": "zh-TW"
    },
    "performanceOptimization": {
      "multiThreading": true,
      "memoryAllocation": "dynamic"
    },
    "environmentVariables": {
      "LIB_PATH": "/usr/local/lib/customLibs",
      "CONFIG_PATH": "/etc/pointCloudApp/config"
    },
    "systemSecurity": {
      "filePermissions": "755",
      "encryption": {
        "enabled": true,
        "type": "AES-256"
      }
    },
    "performanceMonitoring": {
      "enabled": true,
      "loggingLevel": "INFO"
    },
    "multiEnvironment": {
      "development": {
        "debugMode": true,
        "loggingLevel": "DEBUG"
      },
      "production": {
        "debugMode": false,
        "loggingLevel": "ERROR"
      }
    }
  }
  